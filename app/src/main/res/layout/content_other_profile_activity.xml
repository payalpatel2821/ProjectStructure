<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <RelativeLayout
        android:id="@+id/bottom_sheet"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:behavior_hideable="true"
        app:layout_behavior="com.google.android.material.bottomsheet.BottomSheetBehavior"
        tools:context="com.task.newapp.ui.activities.profile.OtherUserProfileActivity">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/_90sdp">

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_90sdp"
                    android:rotation="180"
                    android:scaleType="centerCrop"
                    android:src="@drawable/drop_shadow"
                    app:tint="@color/black" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_90sdp"
                    android:orientation="horizontal"
                    android:padding="@dimen/_5sdp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1.3"
                        android:gravity="center"
                        android:orientation="vertical" />

                    <LinearLayout
                        android:id="@+id/ll_post"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:onClick="onClick"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/txt_post_count"
                            style="@style/styleTextViewActionbar"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="0"
                            android:textColor="@color/white" />

                        <TextView
                            style="@style/styleTextViewNormal"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/post"
                            android:layout_margin="@dimen/_3sdp"
                            android:textColor="@color/white" />

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/ll_follower"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:onClick="onClick"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/txt_follower_count"
                            style="@style/styleTextViewActionbar"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="0"
                            android:textColor="@color/white" />

                        <TextView
                            style="@style/styleTextViewNormal"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/followers"
                            android:layout_margin="@dimen/_3sdp"
                            android:textColor="@color/white" />

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/ll_following"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:onClick="onClick"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/txt_following_count"
                            style="@style/styleTextViewActionbar"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="0"
                            android:textColor="@color/white" />

                        <TextView
                            style="@style/styleTextViewNormal"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/following"
                            android:layout_margin="@dimen/_3sdp"
                            android:textColor="@color/white" />

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1.3"
                        android:gravity="center"
                        android:orientation="vertical" />

                </LinearLayout>

            </RelativeLayout>

            <androidx.appcompat.widget.LinearLayoutCompat
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="-20dp"
                android:clickable="true"
                android:background="@drawable/bg_bottomsheet"
                android:backgroundTint="@color/bottomsheet_bg"
                android:orientation="vertical"
                android:padding="@dimen/_5sdp">

                <View
                    android:id="@+id/viewaa"
                    android:layout_width="@dimen/_40sdp"
                    android:layout_height="@dimen/_2sdp"
                    android:layout_gravity="center"
                    android:layout_marginTop="@dimen/_8sdp"
                    android:layout_marginBottom="@dimen/_8sdp"
                    android:background="@color/gray" />

                <androidx.core.widget.NestedScrollView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_vertical"
                                android:layout_weight="1"
                                android:orientation="vertical">

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center_vertical"
                                    android:gravity="center_vertical"
                                    android:orientation="horizontal"
                                    android:paddingLeft="@dimen/_10sdp">

                                    <androidx.appcompat.widget.AppCompatTextView
                                        android:id="@+id/txt_username"
                                        style="@style/styleTextViewActionbar"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center_vertical"
                                        android:layout_margin="@dimen/_3sdp"
                                        android:layout_weight="1"
                                        android:gravity="center_vertical"
                                        android:singleLine="true"
                                        tools:text="Prinsa adbiz Directory" />


                                </LinearLayout>

                                <androidx.appcompat.widget.AppCompatTextView
                                    android:id="@+id/txt_acc_id"
                                    style="@style/styleTextViewSmall"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginVertical="@dimen/_2sdp"
                                    android:layout_marginHorizontal="@dimen/_3sdp"
                                    android:paddingLeft="@dimen/_10sdp"
                                    tools:text="prinsa25"
                                    android:textColor="@color/darkgray" />

                                <androidx.appcompat.widget.AppCompatTextView
                                    android:id="@+id/txt_status"
                                    style="@style/styleTextViewNormal"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginVertical="@dimen/_7sdp"
                                    android:layout_marginHorizontal="@dimen/_3sdp"
                                    android:paddingLeft="@dimen/_10sdp"
                                    tools:text="Hi,i am using HOW"
                                    />


                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="vertical"
                                android:layout_gravity="center">

                                <androidx.appcompat.widget.AppCompatButton
                                    android:id="@+id/btn_follow"
                                    style="@style/styleCommonButton"
                                    android:layout_width="@dimen/_80sdp"
                                    android:layout_height="@dimen/_30sdp"
                                    android:layout_margin="@dimen/_5sdp"
                                    android:background="@drawable/btn_follow_user"
                                    android:onClick="onClickTag"
                                    android:padding="@dimen/_5sdp"
                                    android:textColor="@color/black"
                                    />

                                <androidx.appcompat.widget.AppCompatButton
                                    android:id="@+id/btn_message1"
                                    style="@style/styleCommonButton"
                                    android:layout_width="@dimen/_80sdp"
                                    android:layout_height="@dimen/_30sdp"
                                    android:layout_margin="@dimen/_5sdp"
                                    android:background="@drawable/btn_rect_rounded_bg"
                                    android:backgroundTint="@color/white"
                                    android:onClick="onClickTag"
                                    android:text="@string/message"
                                    android:textColor="@color/black"
                                    android:visibility="gone"
                                     />
                                <androidx.appcompat.widget.AppCompatTextView
                                    android:id="@+id/txt_lastseen"
                                    style="@style/styleTextViewSmall"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center"
                                    android:drawableLeft="@drawable/ic_time"
                                    android:drawablePadding="@dimen/_2sdp"
                                    android:drawableTint="@color/darkgray"
                                    android:gravity="center_vertical"
                                    android:singleLine="true"
                                    android:layout_marginVertical="@dimen/_5sdp"
                                    android:textColor="@color/darkgray"
                                    tools:text="Yesterday" />
                            </LinearLayout>
                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/ll_imagebutton"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_marginTop="@dimen/_10sdp"
                            android:paddingTop="@dimen/_2sdp"
                            android:paddingBottom="@dimen/_2sdp">

                            <androidx.appcompat.widget.AppCompatImageView
                                android:id="@+id/btn_message"
                                style="@style/styleTextViewSmall"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_gravity="center"
                                android:layout_marginStart="@dimen/_6sdp"
                                android:layout_marginEnd="@dimen/_4sdp"
                                android:layout_weight="1"
                                android:background="@drawable/rect_rounded_bg_white"
                                android:drawablePadding="@dimen/_2sdp"
                                android:padding="@dimen/_10sdp"
                                android:scaleType="centerInside"
                                android:src="@drawable/ic_message_menu"
                                android:text="@string/video"
                                android:textAllCaps="false"
                                android:textColor="@color/black"
                                android:onClick="onClick"
                                android:tint="@color/black" />

                            <androidx.appcompat.widget.AppCompatImageView
                                android:id="@+id/btn_call"
                                style="@style/styleTextViewSmall"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_gravity="center"
                                android:layout_marginStart="@dimen/_6sdp"
                                android:layout_marginEnd="@dimen/_4sdp"
                                android:layout_weight="1"
                                android:background="@drawable/rect_rounded_bg_white"
                                android:drawablePadding="@dimen/_2sdp"
                                android:padding="@dimen/_10sdp"
                                android:scaleType="centerInside"
                                android:src="@drawable/ic_audio_call"
                                android:text="@string/audio"
                                android:textAllCaps="false"
                                android:textColor="@color/black"
                                android:tint="@color/black" />

                            <androidx.appcompat.widget.AppCompatImageView
                                android:id="@+id/btn_mute"
                                style="@style/styleTextViewSmall"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_gravity="center"
                                android:layout_marginStart="@dimen/_6sdp"
                                android:layout_marginEnd="@dimen/_4sdp"
                                android:layout_weight="1"
                                android:background="@drawable/rect_rounded_bg_white"
                                android:drawablePadding="@dimen/_2sdp"
                                android:padding="@dimen/_10sdp"
                                android:scaleType="centerInside"
                                android:src="@drawable/ic_mute"
                                android:textAllCaps="false"
                                android:textColor="@color/black"
                                android:onClick="onClick"
                                android:tint="@color/black" />

                            <androidx.appcompat.widget.AppCompatImageView
                                android:id="@+id/btn_search"
                                style="@style/styleTextViewSmall"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:layout_marginStart="@dimen/_6sdp"
                                android:layout_marginEnd="@dimen/_4sdp"
                                android:layout_weight="1"
                                android:background="@drawable/rect_rounded_bg_white"
                                android:drawablePadding="@dimen/_2sdp"
                                android:padding="@dimen/_10sdp"
                                android:scaleType="centerInside"
                                android:src="@drawable/ic_search_setting"
                                android:textAllCaps="false"
                                android:textColor="@color/black"
                                android:tint="@color/black" />

                        </LinearLayout>

                        <androidx.cardview.widget.CardView
                            android:id="@+id/cv1"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_5sdp"
                            app:cardBackgroundColor="@color/white"
                            app:cardCornerRadius="@dimen/_10sdp"
                            app:cardElevation="0dp"
                            app:cardUseCompatPadding="true">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="vertical"
                                android:padding="@dimen/_5sdp">

                                <TextView
                                    android:id="@+id/txt_contact_detail"
                                    style="@style/styleTextViewNormal"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:drawableLeft="@drawable/ic_contact_detail_with_bg"
                                    android:drawablePadding="@dimen/_10sdp"
                                    android:gravity="center_vertical"
                                    android:onClick="onClick"
                                    android:padding="@dimen/_7sdp"
                                    android:text="@string/contact_detail"
                                     />

                                <View
                                    android:layout_width="match_parent"
                                    android:layout_height="0.2dp"
                                    android:background="@color/gray" />

                                <TextView
                                    android:id="@+id/txt_secret_chat"
                                    style="@style/styleTextViewNormal"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:drawableLeft="@drawable/ic_secret_chat_with_bg"
                                    android:drawablePadding="@dimen/_10sdp"
                                    android:gravity="center_vertical"
                                    android:onClick="onClick"
                                    android:padding="@dimen/_7sdp"
                                    android:text="@string/secret_chat"
                                    />

                                <View
                                    android:layout_width="match_parent"
                                    android:layout_height="0.2dp"
                                    android:background="@color/gray" />

                                <LinearLayout
                                    android:id="@+id/ll_custom_notification"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center_vertical"
                                    android:onClick="onClick"
                                    android:orientation="horizontal">

                                    <TextView
                                        android:id="@+id/txt_custom_notification"
                                        style="@style/styleTextViewNormal"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_centerVertical="true"
                                        android:layout_weight="1"
                                        android:drawableLeft="@drawable/ic_custom_notification_with_bg"
                                        android:drawablePadding="@dimen/_10sdp"
                                        android:gravity="center_vertical"
                                        android:padding="@dimen/_7sdp"
                                        android:text="@string/custom_notification"
                                         />

                                    <TextView
                                        android:id="@+id/txt_custom_notification_status"
                                        style="@style/styleTextViewNormal"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:padding="@dimen/_5sdp"
                                        android:text="on"
                                         />

                                </LinearLayout>

                            </LinearLayout>

                        </androidx.cardview.widget.CardView>

                        <androidx.cardview.widget.CardView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            app:cardBackgroundColor="@color/white"
                            app:cardCornerRadius="@dimen/_10sdp"
                            app:cardElevation="0dp"
                            app:cardUseCompatPadding="true">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="vertical"
                                android:padding="@dimen/_5sdp">


                                <RelativeLayout
                                    android:id="@+id/rl_p_w_l_d"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:gravity="center_vertical">

                                    <TextView
                                        style="@style/styleTextViewNormal"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:gravity="center_vertical"
                                        android:padding="@dimen/_9sdp"
                                        android:text="@string/photo_video_link_docs" />

                                    <TextView
                                        android:id="@+id/txt_pwld_count"
                                        style="@style/styleTextViewSmall"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_alignParentRight="true"
                                        android:layout_centerVertical="true"
                                        android:drawableRight="@drawable/ic_next_resize"
                                        android:drawablePadding="@dimen/_5sdp"
                                        android:drawableTint="@color/gray4"
                                        android:gravity="center"
                                        android:padding="@dimen/_9sdp" />
                                </RelativeLayout>

                                <View
                                    android:layout_width="match_parent"
                                    android:layout_height="0.2dp"
                                    android:background="@color/gray" />

                                <RelativeLayout
                                    android:id="@+id/rl_star"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content">

                                    <TextView
                                        style="@style/styleTextViewNormal"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_centerVertical="true"
                                        android:padding="@dimen/_9sdp"
                                        android:text="@string/star_message" />

                                    <TextView
                                        android:id="@+id/txt_star_count"
                                        style="@style/styleTextViewSmall"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_alignParentRight="true"
                                        android:layout_centerVertical="true"
                                        android:drawableRight="@drawable/ic_next_resize"
                                        android:drawablePadding="@dimen/_5sdp"
                                        android:drawableTint="@color/gray4"
                                        android:gravity="center"
                                        android:padding="@dimen/_9sdp" />
                                </RelativeLayout>

                                <View
                                    android:layout_width="match_parent"
                                    android:layout_height="0.2dp"
                                    android:background="@color/gray" />

                                <RelativeLayout
                                    android:id="@+id/rl_posts"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:onClick="onClick">

                                    <androidx.appcompat.widget.AppCompatTextView
                                        style="@style/styleTextViewNormal"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_centerVertical="true"
                                        android:padding="@dimen/_9sdp"
                                        android:text="@string/post" />

                                    <androidx.appcompat.widget.AppCompatTextView
                                        android:id="@+id/txt_posts_count"
                                        style="@style/styleTextViewSmall"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_alignParentRight="true"
                                        android:layout_centerVertical="true"
                                        android:drawableRight="@drawable/ic_next_resize"
                                        android:drawablePadding="@dimen/_5sdp"
                                        android:drawableTint="@color/gray4"
                                        android:gravity="center"
                                        android:padding="@dimen/_9sdp"
                                        android:text="0" />
                                </RelativeLayout>

                            </LinearLayout>
                        </androidx.cardview.widget.CardView>

                        <androidx.cardview.widget.CardView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            app:cardBackgroundColor="@color/white"
                            app:cardCornerRadius="@dimen/_10sdp"
                            app:cardElevation="0dp"
                            app:cardUseCompatPadding="true">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="vertical"
                                android:padding="@dimen/_5sdp">


                                <RelativeLayout
                                    android:id="@+id/rl_grps"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:onClick="onClick">

                                    <TextView
                                        style="@style/styleTextViewNormal"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:padding="@dimen/_9sdp"
                                        android:text="@string/groups_in_common" />

                                    <androidx.appcompat.widget.AppCompatTextView
                                        android:id="@+id/txt_grpcommon_count"
                                        style="@style/styleTextViewSmall"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_alignParentRight="true"
                                        android:drawableRight="@drawable/ic_next_resize"
                                        android:drawablePadding="@dimen/_5sdp"
                                        android:drawableTint="@color/gray4"
                                        android:gravity="center"
                                        android:padding="@dimen/_9sdp"
                                        android:text="0" />
                                </RelativeLayout>

                                <View
                                    android:layout_width="match_parent"
                                    android:layout_height="0.2dp"
                                    android:background="@color/gray"
                                    android:visibility="gone"/>


                                <RelativeLayout
                                    android:id="@+id/rl_pages"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:onClick="onClick"
                                    android:visibility="gone">

                                    <TextView
                                        style="@style/styleTextViewNormal"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:padding="@dimen/_9sdp"
                                        android:text="@string/pages_in_common" />

                                    <TextView
                                        android:id="@+id/txt_pagescommon_count"
                                        style="@style/styleTextViewSmall"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_alignParentRight="true"
                                        android:drawableRight="@drawable/ic_next_resize"
                                        android:drawablePadding="@dimen/_5sdp"
                                        android:drawableTint="@color/gray4"
                                        android:gravity="center"
                                        android:padding="@dimen/_9sdp"
                                        android:text="0" />
                                </RelativeLayout>

                                <View
                                    android:layout_width="match_parent"
                                    android:layout_height="0.2dp"
                                    android:background="@color/gray"
                                    android:visibility="gone"/>


                                <RelativeLayout
                                    android:id="@+id/rl_frds"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:onClick="onClick"
                                    android:visibility="gone">

                                    <TextView
                                        style="@style/styleTextViewNormal"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:padding="@dimen/_9sdp"
                                        android:text="@string/friends_in_common" />

                                    <TextView
                                        android:id="@+id/txt_frdscommon_count"
                                        style="@style/styleTextViewSmall"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_alignParentRight="true"
                                        android:drawableRight="@drawable/ic_next_resize"
                                        android:drawablePadding="@dimen/_5sdp"
                                        android:drawableTint="@color/gray4"
                                        android:padding="@dimen/_9sdp"
                                        android:gravity="center"
                                        android:text="0" />
                                </RelativeLayout>
                            </LinearLayout>
                        </androidx.cardview.widget.CardView>

                        <androidx.cardview.widget.CardView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            app:cardBackgroundColor="@color/white"
                            app:cardCornerRadius="@dimen/_10sdp"
                            app:cardElevation="0dp"
                            app:cardUseCompatPadding="true">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="vertical"
                                android:padding="@dimen/_5sdp">

                                <TextView
                                    android:id="@+id/txt_block"
                                    style="@style/styleTextViewNormal"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:drawableLeft="@drawable/ic_block"
                                    android:drawablePadding="@dimen/_5sdp"
                                    android:drawableTint="@color/red"
                                    android:onClick="onClick"
                                    android:padding="@dimen/_9sdp"
                                    android:text="@string/block"
                                    android:gravity="center_vertical"
                                    android:textColor="@color/red" />

                                <View
                                    android:layout_width="match_parent"
                                    android:layout_height="0.2dp"
                                    android:background="@color/gray" />

                                <TextView
                                    android:id="@+id/txt_report"
                                    style="@style/styleTextViewNormal"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:drawableLeft="@drawable/ic_report"
                                    android:drawablePadding="@dimen/_5sdp"
                                    android:drawableTint="@color/red"
                                    android:onClick="onClick"
                                    android:padding="@dimen/_9sdp"
                                    android:text="@string/report"
                                    android:gravity="center_vertical"
                                    android:textColor="@color/red" />

                            </LinearLayout>

                        </androidx.cardview.widget.CardView>

                    </LinearLayout>

                </androidx.core.widget.NestedScrollView>

            </androidx.appcompat.widget.LinearLayoutCompat>

        </LinearLayout>

    </RelativeLayout>
</layout>